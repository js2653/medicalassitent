@model IEnumerable<medicalassitent.web.Data.Entities.DocumentType>

<h2>Index</h2>
<p>
    <a class="btn btn-info" asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TipoDeDocumento)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TipoDeDocumento)
            </td>
            <td>
                <a class="btn badge-primary" asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a class="btn badge-secondary" asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a class="btn badge-danger" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
